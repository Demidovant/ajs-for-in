{
  "name": "ajs-for-in",
  "version": "1.0.0",
  "description": "**Важно**: каждая задача выполняется в виде отдельного проекта с собственным GitHub репозиторием.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "jest",
    "lint": " eslint . --fix",
    "build": "webpack --mode production",
    "coverage": "jest --coverage",
    "preshow:coverage": "npm run coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "preshow:dist": "npm run build",
    "show:dist": "live-server dist",
    "prettier": "prettier  — write src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Demidovant/ajs-for-in.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Demidovant/ajs-for-in/issues"
  },
  "homepage": "https://github.com/Demidovant/ajs-for-in#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "transform": {
    "\\.js$": "<rootDir>/node_modules/babel-jest"
  },
  "dependencies": {
    "core-js": "^3.35.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
